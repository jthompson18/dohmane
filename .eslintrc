{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "spread": true,
        "experimentalObjectRestSpread": true,
        "restParams": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "strict": 0,
        "quotes": [2, "single"]
    }
}
